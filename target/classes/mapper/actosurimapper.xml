<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.onlineTicketingSystem.dao.ActosuriDao">

    <select id="findAllActosuriByID" resultType="String">
        select actor from actosuri where id=#{id};
    </select>


    <delete id="deleteById">
        delete from actosuri where id=#{id};
    </delete>

    <select id="insertById">
        insert into actosuri values
        <foreach collection="list" item="actor">
            (#{id},#{actor})
        </foreach>
    </select>

</mapper>